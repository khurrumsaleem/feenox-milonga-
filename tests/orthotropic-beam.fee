PROBLEM mechanical
READ_MESH beam-cantilever-$1.msh

BC left  fixed
BC right tz=-1

Ex = 100000
Ey = 75000
Ez = 50000

Gxy = 30000.0
Gyz = 40000.0
Gzx = 35000.0

nuxy = 0.3
nuyz = 0.25
nu13 = 0.4
nuzx = nu13 * Ez/Ex

SOLVE_PROBLEM

# WRITE_MESH orthotropic-beam.vtk VECTOR u v w sigmax sigmay sigmaz tauxy tauyz tauzx
b = 0.05;
h = 0.02;
PRINT SEP " " %.2f sigmax(0,b/2,h/2) sigmay(0,b/2,h/2) sigmaz(0,b/2,h/2) tauxy(0,b/2,h/2) tauzx(0,b/2,h/2)

